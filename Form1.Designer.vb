    Private Sub btnCompute_Click(sender As Object, e As EventArgs) Handles btnCompute.Click
        Dim intTotal As Integer
        Dim dblMean As Double
        intTotal = CInt(txtEnglish.Text + txtKiswahili.Text + txtMathematics.Text + txtScience.Text + txtSocialScience.Text)
        ' Integer.TryParse(txtMathematics.Text, intEScore)
        dblMean = intTotal / 5
        lblTotalMarks.Text = intTotal.ToString
        lblMeanScore.Text = CInt(dblMean).ToString
        If CInt(dblMean) > 0 And CInt(dblMean) < 40 Then
            lblMeanGrade.Text = "E"
        ElseIf CInt(dblMean) >= 40 And CInt(dblMean) < 50 Then
            lblMeanGrade.Text = "D"
        ElseIf CInt(dblMean) >= 50 And CInt(dblMean) < 60 Then
            lblMeanGrade.Text = "C"
        ElseIf CInt(dblMean) >= 60 And CInt(dblMean) < 70 Then
            lblMeanGrade.Text = "B"
        ElseIf CInt(dblMean) >= 70 And CInt(dblMean) <= 100 Then
            lblMeanGrade.Text = "A"
        Else
            MessageBox.Show("INVALID INPUT")
        End If
    End Sub

