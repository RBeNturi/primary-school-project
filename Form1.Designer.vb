    Private Sub btnSave_Click(sender As Object, e As EventArgs) Handles btnSave.Click
        MessageBox.Show("SUCCESSFULLY RECORDED , ENTER NEXT DETAILS ")
        Call btnClear_Click(sender, e)
    End Sub

    Private Sub btnClear_Click(sender As Object, e As EventArgs) Handles btnClear.Click
        txtEnglish.Clear()
        txtKiswahili.Clear()
        txtMathematics.Clear()
        txtScience.Clear()
        txtSocialScience.Clear()
        lblEGrade.Text = "E"
        lblKGrade.Text = "E"
        lblMGrade.Text = "E"
        lblSGrade.Text = "E"
        lblSSGrade.Text = "E"
        lblMeanGrade.Text = "E"
        lblTotalMarks.Text = "000"
        lblMeanScore.Text = "0.00"
    End Sub

    Private Sub txtEnglish_TextChanged(sender As Object, e As EventArgs) Handles txtEnglish.TextChanged
        Dim intScore As Integer
        Integer.TryParse(txtEnglish.Text, intScore)
        If intScore > 0 And intScore < 40 Then
            lblEGrade.Text = "E"
        ElseIf intScore >= 40 And intScore < 50 Then
            lblEGrade.Text = "D"
        ElseIf intScore >= 50 And intScore < 60 Then
            lblEGrade.Text = "C"
        ElseIf intScore >= 60 And intScore < 70 Then
            lblEGrade.Text = "B"
        ElseIf intScore >= 70 And intScore <= 100 Then
            lblEGrade.Text = "A"
        Else
            If IsNumeric(txtEnglish.Text) = False Or IsNothing(txtEnglish.Text) = True Then
                MessageBox.Show("Please enter a numeric value", "Text only", MessageBoxButtons.OK)
                Me.txtEnglish.Focus()
                'MessageBox.Show("INVALID INPUT")
            End If
        End If
    End Sub

